      d.price = price;
      d.currency = app_currency;
      
      if(price <= 0){
        $(".addToCart-btn, .BuyNowButton").hide();
        $("#floating_action-bar").hide();
        $(".product-single__quantity").addClass('is-hide');    
      }else{
        $(".addToCart-btn, .BuyNowButton").show();  
